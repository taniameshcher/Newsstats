@using WebApplication1.Model;
@{
    ViewData["Title"] = "GoogleNews in USA";
}
<h2>@ViewData["Title"]</h2>
<table class="cinereousTable">
    <tr>
        <th><p align="center">Published at</p></th>
        <th><p align="center">Source name</p></th>
        <th><p align="center">Title</p></th>
        <th><p align="center">Description</p></th>
        <th><p align="center">Num</p></th>
    </tr>
    @{
        foreach (string key in SumAccumulation.CacheDictionary.Keys)
        {
            var article = SumAccumulation.CacheDictionary[key].Article;
            <tr>
                <td style="padding:4px"><p>@article.publishedAt</p></td>
                <td style="padding:4px"><p>@article.source.name</p></td>
                <td style="padding:4px"><p>@article.title</p></td>
                <td style="padding:4px"><p>@article.description</p></td>
                <td style="padding:4px"><p>@SumAccumulation.CacheDictionary[article.url].Count</p></td>
            </tr>
        }
    }
</table>
<h3>Total number of word "@ViewData["keyword"]": @SumAccumulation.Sum</h3>
